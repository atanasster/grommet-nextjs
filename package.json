{
  "name": "grommet-nextjs",
  "version": "1.15.0",
  "engines": {
    "node": "12.10.0",
    "npm": ">=6.0.1"
  },
  "keywords": [
    "grommet",
    "nextjs",
    "react"
  ],
  "scripts": {
    "dev": "PORT=8444 ts-node --project tsconfig.server.json server/server.ts ",
    "alias": "NODE_ENV=alias PORT=8444 ts-node --project tsconfig.server.json server/server.ts ",
    "grommet": "NODE_ENV=grommet PORT=8444 ts-node --project tsconfig.server.json server/server.ts ",
    "dx-grid": "NODE_ENV=dx-grid PORT=8444 ts-node --project tsconfig.server.json server/server.ts ",
    "build": "next build && tsc --project tsconfig.server.json",
    "check": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx .",
    "heroku-postbuild": "npm run build",
    "analyze": "ANALYZE_BUILD=true npm run build",
    "start": "NODE_ENV=production node dist/server/server.js",
    "start-server": "NODE_ENV=production PORT=8444 node dist/server/server.js"
  },
  "dependencies": {
    "@devexpress/dx-react-core": "^2.5.1",
    "@devexpress/dx-react-grid": "^2.5.1",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "apicache": "^1.2.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "1.7.1",
    "chalk": "^2.3.2",
    "chart.js": "^2.7.2",
    "cheerio": "^1.0.0-rc.2",
    "color": "^3.0.0",
    "color-parse": "^1.3.7",
    "color-scheme": "^1.0.0",
    "compression": "^1.7.3",
    "dotenv": "^5.0.1",
    "dotenv-webpack": "^1.5.5",
    "dx-react-grid-grommet": "^1.0.18",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "grommet": "^2.7.10",
    "grommet-controls": "^1.1.21",
    "grommet-icons": "^4.3.0",
    "grommet-theme-aruba": "latest",
    "grommet-theme-dxc": "latest",
    "grommet-theme-hp": "latest",
    "grommet-theme-hpe": "latest",
    "grommet-theme-v1": "latest",
    "isomorphic-unfetch": "^2.0.0",
    "jotform-css.js": "^1.0.1",
    "jss": "^9.8.7",
    "memory-cache": "^0.2.0",
    "next": "^9.1.1",
    "next-routes": "^1.4.2",
    "next-transpile-modules": "^2.3.1",
    "npm": "^6.10.3",
    "numeral": "^2.0.6",
    "react": "^16.8.1",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.8.1",
    "react-ga": "^2.5.0",
    "react-json-pretty": "^1.7.6",
    "react-live": "^1.11.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^4.1.3",
    "url-search-params": "^0.10.0",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-transform-imports": "^1.5.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "pre-commit": "^1.2.2",
    "typescript": "^3.5.3"
  },
  "pre-commit": [
    "check"
  ],
  "license": "ISC"
}
