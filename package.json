{
  "name": "grommet-nextjs",
  "version": "1.10.1",
  "engines": {
    "node": ">=7.6.0",
    "npm": ">=4"
  },
  "keywords": [
    "grommet",
    "nextjs",
    "react"
  ],
  "scripts": {
    "dev": "next dev -p 8444",
    "alias": "NODE_ENV=alias next dev -p 8444",
    "build": "NODE_ENV=production next build",
    "check": "./node_modules/.bin/eslint .",
    "heroku-postbuild": "npm run build",
    "analyze": "ANALYZE_BUILD=true npm run build",
    "start": "NODE_ENV=production next start -p ${PORT}"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-styled-components": "^1.5.1",
    "chalk": "^2.3.2",
    "color": "^3.0.0",
    "color-scheme": "^1.0.0",
    "dotenv": "^5.0.1",
    "dotenv-webpack": "^1.5.5",
    "grommet": "https://github.com/grommet/grommet/tarball/NEXT-stable",
    "grommet-controls": "^0.31.1",
    "grommet-icons": "https://github.com/grommet/grommet-icons/tarball/stable",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^6.1.1",
    "npm": "^5.7.1",
    "numeral": "^2.0.6",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-formify": "^0.10.2",
    "react-ga": "^2.5.0",
    "react-json-pretty": "^1.7.6",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.3.0",
    "url-search-params": "^0.10.0",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-imports": "^1.5.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-parallel": "^0.3.2",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "pre-commit": "^1.2.2"
  },
  "pre-commit": [
    "check"
  ],
  "license": "ISC"
}
